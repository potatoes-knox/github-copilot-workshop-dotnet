{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "nuget",
    "github-actions"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "prCreation": "immediate",
  "separateMajorMinor": true,
  "rangeStrategy": "replace",
  "labels": [
    "dependencies",
    "workshop"
  ]
}
